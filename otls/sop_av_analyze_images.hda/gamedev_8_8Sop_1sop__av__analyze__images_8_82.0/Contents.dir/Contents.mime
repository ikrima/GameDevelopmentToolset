Content-Disposition: attachment; filename="hdaroot/topnet1/pythonscript2.spareparmdef"
Content-Type: text/plain

    group {
        name    "folder1"
        label   "Script"

        parm {
            name    "pdg_workitemgeneration"
            baseparm
            label   "Work Item Generation"
            export  none
        }
        parm {
            name    "inprocess"
            baseparm
            label   "Evaluate In Process"
            export  none
        }
        parm {
            name    "pythonbin"
            baseparm
            label   "Python Bin"
            export  none
        }
        parm {
            name    "python"
            baseparm
            label   "Custom Python Bin"
            export  none
        }
        parm {
            name    "extraargs"
            baseparm
            label   "Extra Arguments"
            export  none
        }
        parm {
            name    "script"
            baseparm
            label   "Script"
            export  none
        }
    }

    parm {
        name    "newparameter"
        label   "Label"
        type    toggle
        default { "0" }
    }
    group {
        name    "folder2"
        label   "Schedulers"
        grouptag { "pdg::nocopy" "" }

        parm {
            name    "topscheduler"
            label   "TOP Scheduler Path"
            type    oppath
            default { "" }
            parmtag { "opfilter" "!!TOP!!" }
            parmtag { "oprelative" "." }
            parmtag { "pdg::nocopy" "" }
        }
        group {
            name    "folder3"
            label   "Local Scheduler"
            grouptag { "pdg::nocopy" "" }

            parm {
                name    "localscheduler_override_toggle"
                label   "Override Job Parameters"
                type    toggle
                default { "0" }
                parmtag { "pdg::nocopy" "" }
            }
            group {
                name    "folder4"
                label   "Job Parameters"
                hidewhentab "{ localscheduler_override_toggle == 0 }"
                grouptag { "pdg::nocopy" "" }

                parm {
                    name    "local_single"
                    label   "Single"
                    type    toggle
                    default { "0" }
                    range   { 0 10 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "local_is_CPU_number_set"
                    label   "local_is_CPU_number_set"
                    type    toggle
                    nolabel
                    joinnext
                    default { "0" }
                    range   { 0 1 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "local_CPUs_to_use"
                    label   "CPUs per Task"
                    type    integer
                    default { "1" }
                    disablewhen "{ local_is_CPU_number_set == 0 }"
                    range   { 1! 64 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "local_usehoudinimaxthreads"
                    label   "local_usehoudinimaxthreads"
                    type    toggle
                    nolabel
                    joinnext
                    default { "0" }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "local_houdinimaxthreads"
                    label   "Houdini Max Threads"
                    type    integer
                    default { "0" }
                    disablewhen "{ local_usehoudinimaxthreads == 0 }"
                    range   { 0 10 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                multiparm {
                    name    "local_envmulti"
                    label    "Task Environment"
                    parmtag { "pdg::nocopy" "" }
                    parmtag { "script_callback" "" }
                    parmtag { "script_callback_language" "hscript" }

                    parm {
                        name    "local_envname#"
                        label   "Name"
                        type    string
                        joinnext
                        default { "" }
                        parmtag { "pdg::scheduler" "" }
                        parmtag { "script_callback_language" "hscript" }
                    }
                    parm {
                        name    "local_envvalue#"
                        label   "Value"
                        type    string
                        default { "" }
                        parmtag { "pdg::scheduler" "" }
                        parmtag { "script_callback_language" "hscript" }
                    }
                }

            }

        }

        group {
            name    "folder3_1"
            label   "HQueue Scheduler"
            grouptag { "pdg::nocopy" "" }

            parm {
                name    "hqueuescheduler_override_toggle"
                label   "Override Job Parameters"
                type    toggle
                default { "0" }
                parmtag { "pdg::nocopy" "" }
            }
            group {
                name    "folder6"
                label   "Job Parameters"
                hidewhentab "{ hqueuescheduler_override_toggle == 0 }"
                grouptag { "pdg::nocopy" "" }

                parm {
                    name    "hqueue_priority"
                    label   "Job Priority"
                    type    ordinal
                    default { "5" }
                    menu {
                        [ "entries = []" ]
                        [ "for i in range(0, 11):" ]
                        [ "    entries.append(str(i))" ]
                        [ "    " ]
                        [ "    if i == 0:" ]
                        [ "        entries.append(str(i) + \" (lowest)\")" ]
                        [ "    elif i == 5:" ]
                        [ "        entries.append(str(i) + \" (medium)\")" ]
                        [ "    elif i == 10:" ]
                        [ "        entries.append(str(i) + \" (highest)\")" ]
                        [ "    else:" ]
                        [ "        entries.append(str(i))" ]
                        [ "    " ]
                        [ "return entries" ]
                        language python
                    }
                    range   { 0 1 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "hqueue_assign_to"
                    label   "Assign To"
                    type    ordinal
                    default { "0" }
                    menu {
                        "any"           "Any Client"
                        "clients"       "Listed Clients"
                        "client_groups" "Clients from Listed Groups"
                    }
                    range   { 0 1 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "hqueue_clients"
                    label   "Clients"
                    type    string
                    joinnext
                    default { "" }
                    hidewhen "{ hqueue_assign_to != clients }"
                    range   { 0 1 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "select_clients"
                    label   "Select Clients"
                    type    button
                    default { "1" }
                    hidewhen "{ hqueue_assign_to != clients }"
                    range   { 0 1 }
                    parmtag { "pdg::nocopy" "" }
                    parmtag { "script_callback" "import hqrop,pdg;addr=hou.nodeType(\"Top/hqueuescheduler\").instances()[0].parm('address').eval();hqrop.selectClients(addr, hou.parm(\"hqueue_clients\"))" }
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "hqueue_client_groups"
                    label   "Client Groups"
                    type    string
                    joinnext
                    default { "" }
                    hidewhen "{ hqueue_assign_to != client_groups }"
                    range   { 0 1 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "select_client_groups"
                    label   "Select Groups"
                    type    button
                    default { "1" }
                    hidewhen "{ hqueue_assign_to != client_groups }"
                    range   { 0 1 }
                    parmtag { "pdg::nocopy" "" }
                    parmtag { "script_callback" "import hqrop,pdg;addr=hou.nodeType(\"Top/hqueuescheduler\").instances()[0].parm('address').eval();hqrop.selectClientGroups(addr, hou.parm(\"hqueue_client_groups\"))" }
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "hq_sepparm2"
                    label   "Separator"
                    type    separator
                    default { "" }
                    range   { 0 1 }
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "hqueue_is_CPU_number_set"
                    label   "Set Number of CPUs per Job"
                    type    toggle
                    default { "0" }
                    range   { 0 1 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                parm {
                    name    "hqueue_CPUs_to_use"
                    label   "CPUs per Job"
                    type    integer
                    default { "1" }
                    disablewhen "{ hqueue_is_CPU_number_set == 0 }"
                    range   { 1! 8 }
                    parmtag { "pdg::scheduler" "" }
                    parmtag { "script_callback_language" "hscript" }
                }
                multiparm {
                    name    "hqueue_envmulti"
                    label    "Task Environment"
                    parmtag { "pdg::nocopy" "" }
                    parmtag { "script_callback_language" "hscript" }

                    parm {
                        name    "hqueue_envname#"
                        label   "Name"
                        type    string
                        joinnext
                        default { "" }
                        parmtag { "pdg::scheduler" "" }
                        parmtag { "script_callback_language" "hscript" }
                    }
                    parm {
                        name    "hqueue_envvalue#"
                        label   "Value"
                        type    string
                        default { "" }
                        parmtag { "pdg::scheduler" "" }
                        parmtag { "script_callback_language" "hscript" }
                    }
                }

                groupcollapsible {
                    name    "folder7"
                    label   "Advanced"

                    parm {
                        name    "hqueue_host"
                        label   "Allowed Host"
                        type    string
                        default { "" }
                        help    "Name of host to run on"
                        parmtag { "pdg::scheduler" "" }
                        parmtag { "script_callback_language" "hscript" }
                    }
                    parm {
                        name    "hqueue_tags"
                        label   "Tag List"
                        type    string
                        default { "" }
                        help    "Space-separated list of tags"
                        parmtag { "pdg::scheduler" "" }
                        parmtag { "script_callback_language" "hscript" }
                    }
                    parm {
                        name    "hqueue_conditions"
                        label   "Conditions JSON"
                        type    string
                        default { "" }
                        help    "JSON conditions spec"
                        parmtag { "editor" "1" }
                        parmtag { "editorlang" "python" }
                        parmtag { "pdg::scheduler" "" }
                        parmtag { "script_callback_language" "hscript" }
                    }
                }

            }

        }

    }


